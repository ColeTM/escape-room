<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="349.0" prefWidth="178.0" spacing="20.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.escape.LoginController">
    <children>
        <Label text="LOGIN" />
      <Label text="Enter Username" />
      <TextField promptText="Username:" />
      <Label text="Enter Password" />
      <PasswordField promptText="Password:" />
        <Button fx:id="secondaryButton" onAction="#switchToLanding" text="Log In" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
